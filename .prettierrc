{
  "singleQuote": true,
  "printWidth": 120,
  "semi": true,
  "trailingComma": "all",
  "arrowParens": "always",
  "tabWidth": 4,
  "bracketSpacing": true,
  "importOrder": ["^@(.*)", "^~/(.*)$", "^[./]"],
  "importOrderSeparation": true,
  "jsxSingleQuote": false,
  "endOfLine": "auto",
  "useTabs": false
}
